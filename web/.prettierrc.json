{
	"bracketSpacing": true,
	"singleQuote": false,
	"arrowParens": "avoid",
	"trailingComma": "none",
	"endOfLine": "auto",
	"quoteProps": "consistent",
	"jsxBracketSameLine": true,
	"useTabs": true,
	"tabWidth": 2,
	"overrides": [
		{
			"files": "*.html",
			"options": {
				"parser": "html"
			}
		}
	]
}
